            <version>2.3.0</version>
        </dependency>

        <!-- activiti-engine -->
        <dependency>
            <groupId>org.activiti</groupId>
            <artifactId>activiti-engine</artifactId>
            <version>5.22.0</version>
        </dependency>
        <!-- activiti-spring -->
        <dependency>
            <groupId>org.activiti</groupId>
